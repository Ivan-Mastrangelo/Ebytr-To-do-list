{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["**/models/*.js"],
    "rules": {
        "no-console": "off",    
        "comma-dangle": ["error", "always-multiline"],
        "treatUndefinedAsUnspecified": false
    }
}
